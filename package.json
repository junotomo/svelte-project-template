{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "sprite": "svg-sprite --dest ./src/components/svg/ --svg-rootattrs ./src/assets/svg/sprite.rootattrs.json --config ./src/assets/svg/sprite.config.json ./src/assets/svg/files/*.svg",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "eslint": "^7.11.0",
    "eslint-config-xo": "^0.33.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-plugin-unicorn": "^23.0.0",
    "nodemon": "^2.0.6",
    "rollup": "^2.3.4",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svg-sprite": "^1.5.0"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "sirv-cli": "^1.0.0",
    "svelte-preprocess": "^4.5.2"
  }
